<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.abapp.soundplay.Fragment.FragmentHome"
        android:label="@string/home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_library"
        android:name="com.abapp.soundplay.Fragment.FragmentLibrary"
        android:label="@string/library"
        tools:layout="@layout/fragment_folders" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.abapp.soundplay.Fragment.FragmentSearch"
        android:label="@string/folder"
        tools:layout="@layout/fragment_folders" />


    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.abapp.soundplay.Fragment.FragmentFolders"
        android:label="@string/home"
        tools:layout="@layout/fragment_home" />



</navigation>